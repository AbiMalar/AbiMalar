def product_of_max_min(arr1, arr2):
    max_first = max(arr1)
    min_second = min(arr2)
    return max_first * min_second
if __name__ == "__main__":
    arr1 = list(map(int, input("Enter the elements of the first array separated by space: ").split()))
    arr2 = list(map(int, input("Enter the elements of the second array separated by space: ").split()))
    result = product_of_max_min(arr1, arr2)
    print("Product of max element of first array and min element of second array:", result)
